import logo from './logo.svg';
import './App.css';
import Procheck from './All';
import { useState } from 'react';

function App() {
  let [count,setCount]=useState(5)
  let clk=()=>
  {
    setCount(count+1)
  }
  return (
    <div>
      <button onClick={clk}>Click Me</button>
      <p>Count: {count}</p>
    </div>
  );
}

export default App;
